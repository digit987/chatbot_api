  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "class preprocess_text():\n",
    "    def __init__(self, text):\n",
    "        self.text = text\n",
    "        \n",
    "    def remove_punctuation(self):\n",
    "        return ''.join([char for char in self.text if char not in punctuation])\n",
    "    \n",
    "    def tokenize(self):\n",
    "        return word_tokenize(self.text)\n",
    "    \n",
    "    def to_lowercase(self):\n",
    "        return ''.join([char.lower() for char in self.text])\n",
    "    \n",
    "    def stemming(self):\n",
    "        self.snowball_stemmer = SnowballStemmer('english')\n",
    "        return ' '.join([self.snowball_stemmer.stem(word) for word in self.tokenize()])\n",
    "    \n",
    "    def preprocess_text(self):\n",
    "        self.text = self.remove_punctuation()\n",
    "        self.text = self.to_lowercase()\n",
    "        self.text = self.stemming()\n",
    "        return self.text"
   ]
